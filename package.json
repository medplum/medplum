{
  "name": "root",
  "version": "2.0.28",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "clean": "turbo run clean",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "cyclonedx": "cyclonedx-npm --omit dev --omit optional --omit peer"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@babel/preset-env": "7.22.9",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@cyclonedx/cyclonedx-npm": "1.13.0",
    "@types/jest": "29.5.3",
    "@types/node": "20.4.2",
    "babel-jest": "29.6.1",
    "babel-preset-vite": "1.1.0",
    "cross-env": "7.0.3",
    "esbuild": "0.18.12",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.6.1",
    "jest-environment-jsdom": "29.6.1",
    "npm-check-updates": "16.10.15",
    "nyc": "15.1.0",
    "rimraf": "5.0.1",
    "source-map-explorer": "2.5.3",
    "ts-node": "10.9.1",
    "tslib": "2.6.0",
    "turbo": "1.10.7",
    "typescript": "5.1.6"
  },
  "overrides": {
    "esbuild": "0.18.12",
    "got": "11.8.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "trim": "0.0.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@medplum/eslint-config"
    ],
    "parserOptions": {
      "project": "./tsconfig.base.json"
    }
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  }
}
