{
  "name": "root",
  "version": "5.0.2",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "turbo run build --filter=!@medplum/docs --filter=!./examples/*",
    "build:all": "turbo run build",
    "build:docs": "turbo run build --filter=@medplum/docs",
    "build:fast": "turbo run build --filter=@medplum/app --filter=@medplum/server",
    "clean": "turbo run clean",
    "lint": "turbo run lint --filter=!@medplum/docs",
    "lint:fix": "turbo run lint:fix --filter=!@medplum/docs",
    "prettier": "prettier --write \"**/*.{ts,tsx,cts,mts,js,jsx,cjs,mjs,json}\"",
    "sort-package-json": "sort-package-json package.json \"packages/*/package.json\" \"examples/*/package.json\"",
    "test": "turbo run test --filter=!@medplum/docs"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ],
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "overrides": {
    "@jest/types": "30.2.0",
    "@swc/core": "1.13.5",
    "esbuild": "0.25.11",
    "jest-diff": "30.2.0",
    "jest-message-util": "30.2.0",
    "jest-util": "30.2.0",
    "jest-worker": "30.2.0",
    "react": "19.2.0",
    "react-compiler-runtime": "latest",
    "react-dom": "19.2.0",
    "rollup": "4.44.2",
    "typescript": "5.9.3",
    "vite": "7.1.12",
    "whatwg-url": "14.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/preset-env": "7.28.5",
    "@babel/preset-react": "7.28.5",
    "@babel/preset-typescript": "7.28.5",
    "@fast-check/jest": "2.1.1",
    "@jest/globals": "30.2.0",
    "@jest/types": "30.2.0",
    "@microsoft/api-documenter": "7.27.3",
    "@microsoft/api-extractor": "7.53.3",
    "@types/jest": "30.0.0",
    "@types/node": "22.18.13",
    "babel-jest": "30.2.0",
    "babel-preset-vite": "1.1.3",
    "cross-env": "10.1.0",
    "esbuild": "0.25.11",
    "formidable": "3.5.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "30.2.0",
    "jest-diff": "30.2.0",
    "jest-each": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jest-message-util": "30.2.0",
    "jest-util": "30.2.0",
    "jest-worker": "30.2.0",
    "jsdom": "27.1.0",
    "npm-check-updates": "19.1.2",
    "nyc": "17.1.0",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "4.3.0",
    "rimraf": "6.1.0",
    "sort-package-json": "3.4.0",
    "source-map-explorer": "2.5.3",
    "tslib": "2.8.1",
    "tsx": "4.20.6",
    "turbo": "2.6.0",
    "typescript": "5.9.3",
    "vite": "7.1.12"
  },
  "packageManager": "npm@10.9.4",
  "engines": {
    "node": "^22.18.0 || >=24.2.0"
  }
}
