{
  "testEnvironment": "node",
  "testRunner": "jasmine2",
  "testTimeout": 60000,
  "testSequencer": "<rootDir>/jest.sequencer.js",
  "transform": {
    "^.+\\.(js|ts)$": "babel-jest"
  },
  "moduleFileExtensions": [
    "ts",
    "js",
    "json",
    "node"
  ],
  "testMatch": [
    "**/src/**/*.test.ts"
  ]
}