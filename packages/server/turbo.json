{
  "$schema": "https://turborepo.org/schema.json",
  "extends": ["//"],
  "pipeline": {
    "test:seed": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts"],
      "env": ["SHOULD_RUN_SEED_TEST"]
    },
    "test": {
      "dependsOn": ["test:seed"],
      "outputs": ["coverage/**"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts"]
    }
  }
}
