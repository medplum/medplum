{
  "port": 33001,
  "baseUrl": "${FHIR_SERVER_BASE_URL}",
  "issuer": "${FHIR_SERVER_BASE_URL}/fhir-server",
  "audience": "${FHIR_SERVER_BASE_URL}",
  "jwksUrl": "${FHIR_SERVER_BASE_URL}/.well-known/jwks.json",
  "authorizeUrl": "${FHIR_SERVER_BASE_URL}/oauth2/authorize",
  "tokenUrl": "${FHIR_SERVER_BASE_URL}/oauth2/token",
  "userInfoUrl": "${FHIR_SERVER_BASE_URL}/oauth2/userinfo",
  "appBaseUrl": "https://localhost:41130/",  
  "maxJsonSize": "${BINARY_UPLOAD_LIMIT_SIZE}",
  "maxBatchSize": "${BINARY_UPLOAD_LIMIT_SIZE}",
  "botLambdaRoleArn": "",
  "botLambdaLayerName": "medplum-bot-layer",
  "vmContextBotsEnabled": true,
  "defaultBotRuntimeVersion": "vmcontext",
  "allowedOrigins": "*",
  "introspectionEnabled": true,
  "database": {
    "host": "${PG__HOST}",
    "port": "${PG__PORT}",
    "dbname": "${PG__DB_NAME}",
    "username": "${PG_SUPER_USER}",
    "password": "${PG_SUPER_PASSWORD}",
    "schema": "${FHIR_CUSTOM_SCHEMA}"
  },
  "redis": {
    "host": "${REDIS_HOST}",
    "port": "${REDIS_PORT}",
    "password": "${REDIS_PASSWORD}"
  },
  "bullmq": {
    "removeOnFail": {
      "count": 1
    },
    "removeOnComplete": {
      "count": 1
    }
  },
  "shutdownTimeoutMilliseconds": 30000,
  "logLevel": "DEBUG",
  "logRequests": true,
  "defaultProjectFeatures": [
    "bots"
  ],
  "defaultAuthRateLimit": "${AUTH_RATE_LIMIT}"
}