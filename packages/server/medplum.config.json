{
  "port": 33001,
  "baseUrl": "http://alp-trex.alp.local:33001",
  "issuer": "http://alp-trex.alp.local:33001/fhir-server",
  "audience": "http://alp-trex.alp.local:33001",
  "jwksUrl": "http://alp-trex.alp.local:33001/.well-known/jwks.json",
  "authorizeUrl": "http://alp-trex.alp.local:33001/oauth2/authorize",
  "tokenUrl": "http://alp-trex.alp.local:33001/oauth2/token",
  "userInfoUrl": "http://alp-trex.alp.local:33001/oauth2/userinfo",
  "appBaseUrl": "https://localhost:41130/",  
  "maxJsonSize": "50mb",
  "maxBatchSize": "50mb",
  "botLambdaRoleArn": "",
  "botLambdaLayerName": "medplum-bot-layer",
  "vmContextBotsEnabled": true,
  "defaultBotRuntimeVersion": "vmcontext",
  "allowedOrigins": "*",
  "introspectionEnabled": true,
  "database": {
    "host": "${PG__HOST}",
    "port": "${PG__PORT}",
    "dbname": "${PG__DB_NAME}",
    "username": "${PG_SUPER_USER}",
    "password": "${PG_SUPER_PASSWORD}",
    "schema": "${FHIR_CUSTOM_SCHEMA}"
  },
  "redis": {
    "host": "${REDIS_HOST}",
    "port": "${REDIS_PORT}",
    "password": "${REDIS_PASSWORD}"
  },
  "bullmq": {
    "removeOnFail": {
      "count": 1
    },
    "removeOnComplete": {
      "count": 1
    }
  },
  "shutdownTimeoutMilliseconds": 30000,
  "logLevel": "DEBUG",
  "logRequests": true,
  "defaultProjectFeatures": [
    "bots"
  ]
}