{
  "name": "@medplum/react",
  "version": "3.0.0",
  "description": "Medplum React Component Library",
  "keywords": [
    "medplum",
    "fhir",
    "healthcare",
    "interoperability",
    "json",
    "serialization",
    "hl7",
    "standards",
    "clinical",
    "dstu2",
    "stu3",
    "r4",
    "normative",
    "ui",
    "input",
    "react",
    "react-component"
  ],
  "homepage": "https://www.medplum.com/",
  "bugs": {
    "url": "https://github.com/medplum/medplum/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medplum/medplum.git",
    "directory": "packages/react"
  },
  "license": "Apache-2.0",
  "author": "Medplum <hello@medplum.com>",
  "sideEffects": [
    "*.css"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./styles.css": "./dist/esm/index.css"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/esm/index.d.ts",
  "files": [
    ".env.defaults",
    "dist/cjs",
    "dist/esm"
  ],
  "scripts": {
    "api-extractor": "api-extractor run --local && cp dist/types.d.ts dist/cjs/index.d.ts && cp dist/types.d.ts dist/esm/index.d.ts",
    "build": "npm run clean && tsc && node esbuild.mjs && npm run api-extractor",
    "chromatic": "chromatic --exit-zero-on-changes --build-script-name=storybook --exit-once-uploaded",
    "clean": "rimraf dist storybook-static",
    "dev": "storybook dev -p 6006",
    "storybook": "storybook build",
    "test": "jest"
  },
  "devDependencies": {
    "@mantine/core": "7.4.2",
    "@mantine/hooks": "7.4.2",
    "@mantine/notifications": "7.4.2",
    "@medplum/core": "*",
    "@medplum/definitions": "*",
    "@medplum/fhirtypes": "*",
    "@medplum/mock": "*",
    "@medplum/react-hooks": "*",
    "@storybook/addon-actions": "7.6.9",
    "@storybook/addon-essentials": "7.6.9",
    "@storybook/addon-links": "7.6.9",
    "@storybook/addon-storysource": "7.6.9",
    "@storybook/builder-vite": "7.6.9",
    "@storybook/react": "7.6.9",
    "@storybook/react-vite": "7.6.9",
    "@tabler/icons-react": "2.46.0",
    "@testing-library/dom": "9.3.4",
    "@testing-library/jest-dom": "6.2.0",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@vitejs/plugin-react": "4.2.1",
    "chromatic": "10.3.1",
    "jest": "29.7.0",
    "jest-each": "29.7.0",
    "postcss": "8.4.33",
    "postcss-preset-mantine": "1.12.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rfc6902": "5.1.1",
    "rimraf": "5.0.5",
    "sinon": "17.0.1",
    "storybook": "7.6.9",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "@mantine/core": "^7.0.0",
    "@mantine/hooks": "^7.0.0",
    "@mantine/notifications": "^7.0.0",
    "@medplum/core": "*",
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0",
    "rfc6902": "^5.0.1"
  },
  "peerDependenciesMeta": {
    "@mantine/core": {
      "optional": true
    },
    "@mantine/hooks": {
      "optional": true
    },
    "@mantine/notifications": {
      "optional": true
    },
    "rfc6902": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
