{
  "name": "medplum-patient-intake-demo",
  "version": "4.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build:bots && tsc && vite build",
    "build:bots": "npm run clean && tsc --project tsconfig-bots.json && node --no-warnings esbuild-script.mjs && node --loader ts-node/esm src/scripts/deploy-bots.ts",
    "clean": "rimraf dist",
    "dev": "vite",
    "lint": "eslint src/",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "@medplum/eslint-config"
    ]
  },
  "devDependencies": {
    "@mantine/core": "7.17.5",
    "@mantine/hooks": "7.17.5",
    "@mantine/notifications": "7.17.5",
    "@medplum/bot-layer": "4.1.1",
    "@medplum/core": "4.1.1",
    "@medplum/definitions": "4.1.1",
    "@medplum/eslint-config": "4.1.1",
    "@medplum/fhirtypes": "4.1.1",
    "@medplum/mock": "4.1.1",
    "@medplum/react": "4.1.1",
    "@tabler/icons-react": "3.17.0",
    "@types/node": "20.17.32",
    "@types/react": "18.3.20",
    "@types/react-dom": "18.3.6",
    "@vitejs/plugin-react": "4.4.1",
    "chart.js": "4.4.9",
    "esbuild": "0.25.3",
    "postcss": "8.5.3",
    "postcss-preset-mantine": "1.17.0",
    "react": "18.2.0",
    "react-chartjs-2": "5.3.0",
    "react-dom": "18.2.0",
    "react-router": "7.5.0",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "vite": "6.3.3",
    "vitest": "3.1.2"
  }
}
