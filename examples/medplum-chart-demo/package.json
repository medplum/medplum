{
  "name": "medplum-chart-demo",
  "version": "3.2.19",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build:bots && tsc && vite build",
    "build:bots": "npm run clean && tsc --project tsconfig-bots.json && node --no-warnings esbuild-script.mjs && node --loader ts-node/esm src/scripts/deploy-bots.ts",
    "clean": "rimraf dist",
    "dev": "vite",
    "lint": "eslint src/",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "@medplum/eslint-config"
    ]
  },
  "devDependencies": {
    "@mantine/core": "7.13.4",
    "@mantine/hooks": "7.13.4",
    "@mantine/notifications": "7.13.4",
    "@medplum/bot-layer": "3.2.19",
    "@medplum/core": "3.2.19",
    "@medplum/definitions": "3.2.19",
    "@medplum/eslint-config": "3.2.19",
    "@medplum/fhirtypes": "3.2.19",
    "@medplum/mock": "3.2.19",
    "@medplum/react": "3.2.19",
    "@tabler/icons-react": "3.17.0",
    "@types/node": "22.8.7",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "4.3.3",
    "chart.js": "4.4.6",
    "esbuild": "0.24.0",
    "fast-glob": "3.3.2",
    "postcss": "8.4.47",
    "postcss-preset-mantine": "1.17.0",
    "react": "18.2.0",
    "react-chartjs-2": "5.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.27.0",
    "ts-node": "10.9.2",
    "typescript": "5.6.3",
    "vite": "5.4.10",
    "vitest": "2.1.4"
  }
}
