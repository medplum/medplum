{
  "name": "medplum-chart-demo",
  "version": "3.1.8",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run clean && npm run build:bots && tsc && vite build",
    "build:bots": "npm run clean && tsc --project tsconfig-bots.json && node --no-warnings esbuild-script.mjs && node --loader ts-node/esm src/scripts/deploy-bots.ts",
    "clean": "rimraf dist",
    "lint": "eslint src/",
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "@medplum/eslint-config"
    ]
  },
  "devDependencies": {
    "@mantine/core": "7.10.1",
    "@mantine/hooks": "7.10.1",
    "@mantine/notifications": "7.10.1",
    "@medplum/core": "3.1.8",
    "@medplum/eslint-config": "3.1.8",
    "@medplum/fhirtypes": "3.1.8",
    "@medplum/react": "3.1.8",
    "@tabler/icons-react": "3.5.0",
    "@types/node": "20.14.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.0",
    "chart.js": "4.4.3",
    "esbuild": "0.21.2",
    "postcss": "8.4.38",
    "postcss-preset-mantine": "1.15.0",
    "react": "18.2.0",
    "react-chartjs-2": "5.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.23.1",
    "typescript": "5.4.5",
    "vite": "5.2.12"
  }
}
