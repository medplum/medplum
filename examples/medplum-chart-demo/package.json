{
  "name": "medplum-chart-demo",
  "version": "3.2.3",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build:bots && tsc && vite build",
    "build:bots": "npm run clean && tsc --project tsconfig-bots.json && node --no-warnings esbuild-script.mjs && node --loader ts-node/esm src/scripts/deploy-bots.ts",
    "clean": "rimraf dist",
    "dev": "vite",
    "lint": "eslint src/",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "@medplum/eslint-config"
    ]
  },
  "devDependencies": {
    "@mantine/core": "7.11.2",
    "@mantine/hooks": "7.11.2",
    "@mantine/notifications": "7.11.2",
    "@medplum/core": "3.2.3",
    "@medplum/eslint-config": "3.2.3",
    "@medplum/fhirtypes": "3.2.3",
    "@medplum/react": "3.2.3",
    "@tabler/icons-react": "3.10.0",
    "@types/node": "20.14.10",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.1",
    "chart.js": "4.4.3",
    "esbuild": "0.23.0",
    "fast-glob": "3.3.2",
    "postcss": "8.4.39",
    "postcss-preset-mantine": "1.15.0",
    "react": "18.2.0",
    "react-chartjs-2": "5.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.24.1",
    "typescript": "5.5.3",
    "vite": "5.3.3"
  }
}
