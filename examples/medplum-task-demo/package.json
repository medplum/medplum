{
  "name": "medplum-task-demo",
  "version": "5.0.13",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build:bots && tsc && vite build",
    "build:bots": "npm run clean && tsc && node --no-warnings esbuild-script.mjs && tsx src/scripts/deploy-bots.ts",
    "clean": "rimraf dist data/example/example-bots.json",
    "dev": "npm run build:bots && vite",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "preview": "vite preview",
    "test": "vitest run"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@mantine/core": "8.3.13",
    "@mantine/hooks": "8.3.13",
    "@mantine/notifications": "8.3.13",
    "@medplum/core": "5.0.13",
    "@medplum/definitions": "5.0.13",
    "@medplum/eslint-config": "5.0.13",
    "@medplum/fhirtypes": "5.0.13",
    "@medplum/mock": "5.0.13",
    "@medplum/react": "5.0.13",
    "@tabler/icons-react": "3.36.1",
    "@types/node": "22.19.7",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "esbuild": "0.27.2",
    "postcss": "8.5.6",
    "postcss-preset-mantine": "1.18.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-router": "7.12.0",
    "rimraf": "6.1.2",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vitest": "4.0.18"
  },
  "packageManager": "npm@10.9.4",
  "engines": {
    "node": "^22.18.0 || >=24.2.0"
  }
}
