{
  "name": "medplum-task-demo",
  "version": "5.0.5",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build:bots && tsc && vite build",
    "build:bots": "npm run clean && tsc && node --no-warnings esbuild-script.mjs && tsx src/scripts/deploy-bots.ts",
    "clean": "rimraf dist data/example/example-bots.json",
    "dev": "npm run build:bots && vite",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "preview": "vite preview",
    "test": "vitest run"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@mantine/core": "8.3.7",
    "@mantine/hooks": "8.3.7",
    "@mantine/notifications": "8.3.7",
    "@medplum/core": "5.0.5",
    "@medplum/definitions": "5.0.5",
    "@medplum/eslint-config": "5.0.5",
    "@medplum/fhirtypes": "5.0.5",
    "@medplum/mock": "5.0.5",
    "@medplum/react": "5.0.5",
    "@tabler/icons-react": "3.35.0",
    "@types/node": "22.19.1",
    "@types/react": "19.2.4",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.1",
    "esbuild": "0.27.0",
    "postcss": "8.5.6",
    "postcss-preset-mantine": "1.18.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router": "7.9.6",
    "rimraf": "6.1.0",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "vite": "7.2.2",
    "vitest": "4.0.9"
  },
  "packageManager": "npm@10.9.4",
  "engines": {
    "node": "^22.18.0 || >=24.2.0"
  }
}
