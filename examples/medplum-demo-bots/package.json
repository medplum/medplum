{
  "name": "medplum-demo-bots",
  "version": "2.1.22",
  "description": "Medplum Demo Bots",
  "license": "Apache-2.0",
  "author": "Medplum <hello@medplum.com>",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run lint && tsc && node esbuild-script.mjs",
    "lint": "eslint src/",
    "prettier": "prettier --write .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "@medplum/eslint-config"
    ]
  },
  "dependencies": {
    "esbuild": "0.19.8"
  },
  "devDependencies": {
    "@medplum/cli": "2.1.22",
    "@medplum/core": "2.1.22",
    "@medplum/eslint-config": "2.1.22",
    "@medplum/fhirtypes": "2.1.22",
    "@medplum/mock": "2.1.22",
    "@types/node": "20.10.0",
    "@types/node-fetch": "2.6.9",
    "@types/ssh2-sftp-client": "9.0.3",
    "@vitest/coverage-v8": "0.34.6",
    "@vitest/ui": "0.34.6",
    "form-data": "4.0.0",
    "glob": "^10.3.10",
    "node-fetch": "2.7.0",
    "pdfmake": "0.2.8",
    "ssh2-sftp-client": "9.1.0",
    "stripe": "14.5.0",
    "typescript": "5.3.2",
    "vitest": "0.34.6"
  }
}
