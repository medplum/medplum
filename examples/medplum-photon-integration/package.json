{
  "name": "medplum-photon-integration",
  "version": "3.2.17",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build:bots && tsc && vite build",
    "build:bots": "npm run clean && tsc --project tsconfig-bots.json && node --no-warnings esbuild-script.mjs && node --loader ts-node/esm src/scripts/deploy-bots.ts",
    "clean": "rimraf dist",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "@medplum/eslint-config"
    ]
  },
  "devDependencies": {
    "@mantine/core": "7.13.3",
    "@mantine/hooks": "7.13.3",
    "@mantine/notifications": "7.13.3",
    "@medplum/core": "3.2.17",
    "@medplum/eslint-config": "3.2.17",
    "@medplum/fhirtypes": "3.2.17",
    "@medplum/react": "3.2.17",
    "@photonhealth/elements": "0.12.9",
    "@tabler/icons-react": "3.19.0",
    "@types/node": "22.7.7",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "4.3.3",
    "postcss": "8.4.47",
    "esbuild": "0.24.0",
    "postcss-preset-mantine": "1.17.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.27.0",
    "typescript": "5.6.3",
    "vite": "5.4.9"
  }
}
