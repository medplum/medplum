{
  "name": "medplum-scheduling-demo",
  "version": "4.0.2",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build:bots && tsc && vite build",
    "build:bots": "npm run clean && tsc --project tsconfig-bots.json && node --no-warnings esbuild-script.mjs && node --loader ts-node/esm src/scripts/deploy-bots.ts",
    "clean": "rimraf dist",
    "dev": "vite",
    "lint": "eslint src/",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "@medplum/eslint-config"
    ]
  },
  "devDependencies": {
    "@mantine/core": "7.17.0",
    "@mantine/hooks": "7.17.0",
    "@mantine/notifications": "7.17.0",
    "@medplum/bot-layer": "4.0.2",
    "@medplum/core": "4.0.2",
    "@medplum/definitions": "4.0.2",
    "@medplum/eslint-config": "4.0.2",
    "@medplum/fhirtypes": "4.0.2",
    "@medplum/mock": "4.0.2",
    "@medplum/react": "4.0.2",
    "@tabler/icons-react": "3.17.0",
    "@types/node": "20.17.19",
    "@types/react": "18.3.18",
    "@types/react-big-calendar": "^1.16.1",
    "@types/react-dom": "18.3.5",
    "@vitejs/plugin-react": "4.3.4",
    "chart.js": "4.4.8",
    "esbuild": "0.25.0",
    "postcss": "8.5.3",
    "postcss-preset-mantine": "1.17.0",
    "react": "18.2.0",
    "react-chartjs-2": "5.3.0",
    "react-dom": "18.2.0",
    "ts-node": "10.9.2",
    "vitest": "3.0.7",
    "dayjs": "1.11.13",
    "react-big-calendar": "1.18.0",
    "react-router-dom": "6.29.0",
    "typescript": "5.7.3",
    "vite": "6.2.0"
  }
}
