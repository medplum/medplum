{
  "name": "medplum-scheduling-demo",
  "version": "4.3.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build:bots && tsc && vite build",
    "build:bots": "npm run clean && tsc --project tsconfig-bots.json && node --no-warnings esbuild-script.mjs && node --loader ts-node/esm src/scripts/deploy-bots.ts",
    "clean": "rimraf dist",
    "dev": "vite",
    "lint": "eslint src/",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "@medplum/eslint-config"
    ]
  },
  "devDependencies": {
    "@mantine/core": "7.17.8",
    "@mantine/hooks": "7.17.8",
    "@mantine/notifications": "7.17.8",
    "@medplum/bot-layer": "4.3.0",
    "@medplum/core": "4.3.0",
    "@medplum/definitions": "4.3.0",
    "@medplum/eslint-config": "4.3.0",
    "@medplum/fhirtypes": "4.3.0",
    "@medplum/mock": "4.3.0",
    "@medplum/react": "4.3.0",
    "@tabler/icons-react": "3.17.0",
    "@types/node": "20.19.2",
    "@types/react": "19.1.8",
    "@types/react-big-calendar": "1.16.2",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.6.0",
    "chart.js": "4.5.0",
    "dayjs": "1.11.13",
    "esbuild": "0.25.5",
    "postcss": "8.5.6",
    "postcss-preset-mantine": "1.17.0",
    "react": "19.1.0",
    "react-big-calendar": "1.19.4",
    "react-chartjs-2": "5.3.0",
    "react-dom": "19.1.0",
    "react-router": "7.6.3",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vitest": "3.2.4"
  },
  "packageManager": "npm@10.9.2",
  "engines": {
    "node": ">=20.0.0"
  }
}
