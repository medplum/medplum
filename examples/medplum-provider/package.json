{
  "name": "medplum-provider",
  "version": "4.3.12",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run bots:build && vite build",
    "dev": "vite",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "bots:build": "npm run clean && npm run lint && tsc && node --no-warnings esbuild-script.mjs",
    "bots:deploy": "npx medplum bot deploy $1"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "@medplum/eslint-config"
    ]
  },
  "devDependencies": {
    "@mantine/core": "7.17.8",
    "@mantine/hooks": "7.17.8",
    "@mantine/notifications": "7.17.8",
    "@medplum/bot-layer": "4.3.12",
    "@medplum/cli": "4.3.12",
    "@medplum/core": "4.3.12",
    "@medplum/definitions": "4.3.12",
    "@medplum/dosespot-react": "4.3.12",
    "@medplum/eslint-config": "4.3.12",
    "@medplum/fhirtypes": "4.3.12",
    "@medplum/health-gorilla-core": "4.3.12",
    "@medplum/health-gorilla-react": "4.3.12",
    "@medplum/mock": "4.3.12",
    "@medplum/react": "4.3.12",
    "@tabler/icons-react": "3.34.1",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "20.19.13",
    "@types/react": "19.1.12",
    "@types/react-big-calendar": "1.16.2",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "5.0.2",
    "fast-xml-parser": "5.2.5",
    "dayjs": "1.11.18",
    "jsdom": "26.1.0",
    "jszip": "3.10.1",
    "postcss": "8.5.6",
    "postcss-preset-mantine": "1.18.0",
    "react": "19.1.1",
    "react-big-calendar": "1.19.4",
    "react-dom": "19.1.1",
    "react-router": "7.8.2",
    "rimraf": "6.0.1",
    "typescript": "5.9.2",
    "uuid": "11.1.0",
    "vite": "7.1.5",
    "vitest": "3.2.4"
  },
  "packageManager": "npm@10.9.3",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  }
}
